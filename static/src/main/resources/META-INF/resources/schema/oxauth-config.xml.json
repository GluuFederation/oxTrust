            "minItems": 1,
            "uniqueItems": true,
            "items": {
                "type": "string",
                "enum": [
                    "authorization_code",
                    "implicit",
                    "password",
                    "client_credentials",
                    "refresh_token",
                    "urn:ietf:params:oauth:grant-type:uma-ticket"
                ]
            }
        },
        "dynamicGrantTypeDefault": {
            "id": "dynamicGrantTypeDefault",
